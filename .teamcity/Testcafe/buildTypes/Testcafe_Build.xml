<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="d8c7d741-9903-4f8c-ad4f-cea3ceb16dd7" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/2018.1/project-config.xsd">
  <name>Build</name>
  <description />
  <settings>
    <parameters />
    <build-runners>
      <runner id="RUNNER_3" name="Run TestCafe Tests" type="simpleRunner">
        <parameters>
          <param name="command.executable" value="testcafe%vcsroot.submoduleCheckout%" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_5" name="" type="Xcode">
        <parameters>
          <param name="arch" value="default" />
          <param name="buildActions" value="clean build" />
          <param name="configuration" value="###default###" />
          <param name="customBuildOutputDir" value="build" />
          <param name="platform" value="default" />
          <param name="project" value="tests%teamcity.build.checkoutDir%" />
          <param name="runTests" value="true" />
          <param name="sdk" value="default" />
          <param name="target" value="###default###" />
          <param name="teamcity.step.mode" value="default" />
          <param name="xcode" value="3" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="Test" />
    </vcs-settings>
    <requirements />
    <build-triggers>
      <build-trigger id="vcsTrigger" type="vcsTrigger">
        <parameters>
          <param name="branchFilter" value="+:*" />
          <param name="enableQueueOptimization" value="true" />
          <param name="quietPeriodMode" value="DO_NOT_USE" />
        </parameters>
      </build-trigger>
    </build-triggers>
    <cleanup />
  </settings>
